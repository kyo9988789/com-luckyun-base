<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luckyun.base.notice.mapper.SysNoticeAttachmentMapper">
	
	<select id="findByIlinkno" resultType="com.luckyun.model.notice.SysNoticeAttachment" parameterType="long">
		select a.* from sys_notice_attachment a where a.ilinkno = #{ilinkno}
	</select>
</mapper>